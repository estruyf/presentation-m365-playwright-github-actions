{
  "$schema": "https://elio.dev/demo-time.schema.json",
  "title": "0.defaults",
  "description": "",
  "demos": [
    {
      "title": "üóëÔ∏è Clean up",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.action.terminal.killAll"
        },
        {
          "action": "executeTerminalCommand",
          "command": "rm -rf ./e2e/playwright && rm -rf ./e2e/tests/pages/sharepoint.spec.ts-snapshots && touch ./e2e/.env/.env.variables && touch ./e2e/.env.token && touch ./e2e/.env/.env.account"
        }
      ]
    },
    {
      "title": "ü™ß Open slides",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.action.terminal.killAll"
        },
        {
          "action": "snippet",
          "contentPath": "./defaults/show_slide.json",
          "args": {
            "SLIDES_URL": "http://localhost:3030/"
          }
        }
      ]
    }
  ]
}
